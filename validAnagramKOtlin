fun validAnagram(string1: String, string2: String): Boolean {
    if (string1.size != string2.size) {
        return false
    }

    val frequencyMap1 = mutableMapOf<String, Int>()

    for (letter in string1) {
        frequencyMap1[letter] = frequencyMap1.getOrDefault(letter, 0) + 1
    }

    for ((key, value) in frequencyMap1) {
        if (!frequencyMap2.containsKey(key) || frequencyMap2[key] != value) {
            return false
        }
    }

    return true
}

fun main() {
    val result = validAnagram("omare", "eramo"
    println(result)
}
